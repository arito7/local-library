<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head',{title}) %>
  <body>
    <header><%- include('./partials/navbar') %></header>
    <main>
      <%- include('./partials/sidebar') %>
      <div class="container">
        <h1><%= title %></h1>
        <table>
          <thead>
            <tr>
              <th>Genre</th>
            </tr>
          </thead>
          <tbody>
            <% genre_list.forEach(genre => { %>
            <tr>
              <td><a href="<%= genre.url %>"><%= genre.name %></a></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </main>
    <footer></footer>
  </body>
</html>
